        <head>
            <script>
                 
                 function LimparCPF(CPF) {
                    var CPFLimpo = "";

                    for (var char of CPF){
                        if(char == "0" || char == "1" || char == "2" || char == "3" || char == "4" || char == "5" || char == "6" || char == "7" || char == "8" || char == "9" || );    
                        CPFLimpo = CPFLimpo + char;
                    }
                    console.log("CPF Original: ", CPF);
                    console.log("CPF Modificado: ", CPFLimpo);
}
function VerificarCPF(){
    var CPF = document.getElementById("_cpf").value;
    CPF = LimparCPF(CPF);

    for(var i = 0; i < 9; i++) X = X + (10 - 1) * parseInt(CPF[i]);
    Resto = X % 11;
    if (Resto == 0 || Resto == 1) PrimeiroDigito = 0;
    else PrimeiroDigito = 11 - Resto;
    
    for(var i = 0; i < 9; i++) X = X + (10 - 1) * parseInt(CPF[i]);
    Resto = X % 11;
    if (Resto == 0 || Resto == 1) PrimeiroDigito = 0;
    else PrimeiroDigito = 11 - Resto;
}

            </script>
        </head>
    </body>
